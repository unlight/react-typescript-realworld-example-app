{
  "compilerOptions": {
    "target": "es2017",
    "module": "esnext",
    "jsx": "react",
    "moduleResolution": "node",
    "importHelpers": true,
    "strict": true,
    "noImplicitAny": false,
    "experimentalDecorators": false,
    "emitDecoratorMetadata": false,
    "outDir": "dist",
    "pretty": true,
    "esModuleInterop": true,
    "removeComments": false,
    "sourceMap": true,
    "declaration": true,
    "declarationMap": true,
    "skipLibCheck": true,
    "lib": ["dom", "esnext"],
    "baseUrl": ".",
    "paths": {
      "app_modules/*": ["src/app_modules/*"]
    }
  },
  "include": ["src/**/*.ts", "src/**/*.tsx"]
}
