{
  "name": "react-typescript-realworld-example-app",
  "private": true,
  "version": "0.0.0-dev",
  "license": "MIT",
  "description": "Realworld example app",
  "keywords": [],
  "scripts": {
    "test": "npm run eslint && npm run tscheck && npm run test:cov",
    "test:r": "node node_modules/jest/bin/jest --runInBand --verbose",
    "test:w": "npm run test:r -- --watch",
    "test:cov": "npm run test:r -- --collectCoverage",
    "test:d": "node --inspect-brk node_modules/jest/bin/jest --runInBand --testMatch \"<rootDir>/src/**/*.spec.ts\"",
    "test:m": "node node_modules/stryker-cli/bin/stryker-cli run",
    "typecov": "node node_modules/type-coverage/bin/type-coverage --detail",
    "tscheck": "echo tscheck... && tsc --noEmit",
    "tscheck:w": "npm run tscheck -- --watch",
    "tsclint": "tsc --noEmit --strict --forceConsistentCasingInFileNames --noImplicitReturns --noImplicitThis --noUnusedLocals --noUnusedParameters",
    "tsclint:w": "npm run tsclint -- --watch",
    "eslint:w": "watchexec -w src \"npm run eslint\"",
    "eslint": "node node_modules/eslint/bin/eslint \"src/**/*.{ts,tsx}\"",
    "eslint:fix": "npm run eslint -- --fix",
    "tslint:fix": "node node_modules/tslint/bin/tslint -p tsconfig.json --fix",
    "lint:w:win32": "sh Taskfile lint_watch",
    "lint:w": "run-p tsclint:w eslint:w",
    "lint:fix": "npm run tslint:fix || true && npm run eslint:fix",
    "commit": "node node_modules/cz-customizable/standalone.js",
    "cmlint": "sh Taskfile commit_lint",
    "build:libs": "is-changed -d -t dist/.libs.dat -u \"webpack --env.libs --progress\"",
    "dev": "node node_modules/webpack-dev-server/bin/webpack-dev-server",
    "predev": "npm run build:libs",
    "build": "sh Taskfile build",
    "build:prod": "webpack --mode production",
    "bundle": "sh Taskfile bundle",
    "remark": "sh Taskfile remark_run",
    "remark:update": "sh Taskfile remark_run --output"
  },
  "dependencies": {
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-router-dom": "^5.1.2",
    "tslib": "^1.11.1",
    "unstated-next": "^1.1.0"
  },
  "husky": {
    "hooks": {
      "pre-commit": "precise-commits",
      "pre-push": "npm run test",
      "commit-msg": "git-branch-is -r \"^(release|master|develop)\" && commitlint -E HUSKY_GIT_PARAMS || echo \"Ignoring commitlint error...\""
    }
  },
  "devDependencies": {
    "@commitlint/cli": "^8.3.5",
    "@commitlint/config-conventional": "^8.3.4",
    "@testing-library/react": "^10.0.3",
    "@types/jest": "^25.2.1",
    "@types/node": "^13.13.2",
    "@types/react": "^16.9.34",
    "@types/react-dom": "^16.9.6",
    "@types/react-router-dom": "^5.1.4",
    "@types/testing-library__react": "^10.0.1",
    "@types/webpack": "^4.41.12",
    "@types/webpack-env": "^1.15.2",
    "@typescript-eslint/eslint-plugin": "^2.29.0",
    "@typescript-eslint/eslint-plugin-tslint": "^2.29.0",
    "@typescript-eslint/parser": "^2.29.0",
    "add-asset-html-webpack-plugin": "^3.1.3",
    "cz-customizable": "^6.2.0",
    "eslint": "^6.8.0",
    "eslint-import-resolver-node": "^0.3.3",
    "eslint-plugin-import": "^2.20.2",
    "eslint-plugin-jest": "^23.8.2",
    "eslint-plugin-only-warn": "^1.0.2",
    "eslint-plugin-prettier": "^3.1.3",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-react-hooks": "^3.0.0",
    "eslint-plugin-simple-import-sort": "^5.0.2",
    "eslint-plugin-sonarjs": "^0.5.0",
    "eslint-plugin-unicorn": "^18.0.1",
    "eslint-plugin-wix-editor": "^3.0.0",
    "git-branch-is": "^3.1.0",
    "html-loader": "^1.1.0",
    "html-webpack-plugin": "^4.2.0",
    "husky": "^4.2.5",
    "is-changed": "^3.0.3",
    "istanbul-instrumenter-loader": "^3.0.1",
    "jest": "^25.4.0",
    "npm-run-all": "^4.1.5",
    "precise-commits": "^1.0.2",
    "prettier": "^2.0.5",
    "read-pkg-up": "^7.0.1",
    "remark": "^12.0.0",
    "remark-cli": "^8.0.0",
    "remark-license": "^5.1.1",
    "remark-toc": "^7.0.0",
    "simplytyped": "^3.2.3",
    "source-map-loader": "^0.2.4",
    "terser-webpack-plugin": "^2.3.5",
    "ts-jest": "^25.4.0",
    "ts-loader": "^7.0.1",
    "tsconfig-paths-webpack-plugin": "^3.2.0",
    "tslint": "^6.1.1",
    "tslint-clean-code": "^0.2.10",
    "tslint-consistent-codestyle": "^1.16.0",
    "tslint-etc": "^1.10.1",
    "tslint-microsoft-contrib": "^6.2.0",
    "tslint-sonarts": "^1.9.0",
    "type-coverage": "^2.4.2",
    "typescript": "~3.8.3",
    "watchexec-bin": "^1.0.0",
    "webpack": "^4.43.0",
    "webpack-cli": "^3.3.11",
    "webpack-dev-server": "^3.10.3"
  }
}
