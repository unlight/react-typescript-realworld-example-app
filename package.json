{
  "name": "realworld-example-app",
  "private": true,
  "version": "0.0.0-dev",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "serve": "vite preview",
    "tscheck": "tsc --noEmit",
    "tscheck:w": "npm run tscheck -- --watch",
    "test": "npm run eslint && npm run tscheck && npm run test:cov",
    "test:r": "node node_modules/jest/bin/jest --runInBand --verbose",
    "test:w": "npm run test:r -- --watch",
    "test:cov": "npm run test:r -- --collectCoverage",
    "eslint": "node node_modules/eslint/bin/eslint \"src/**/*.{ts,tsx}\"",
    "eslint:w": "watchexec -w src \"npm run eslint\"",
    "eslint:fix": "npm run eslint -- --fix"
  },
  "dependencies": {
    "react": "^17.0.2",
    "react-dom": "^17.0.2"
  },
  "devDependencies": {
    "@kyleshevlin/eslint-plugin": "^1.3.0",
    "@testing-library/react": "^12.0.0",
    "@types/jest": "^26.0.24",
    "@types/react": "^17.0.14",
    "@types/react-dom": "^17.0.9",
    "@typescript-eslint/eslint-plugin": "^4.28.2",
    "@typescript-eslint/parser": "^4.28.2",
    "@vitejs/plugin-react-refresh": "^1.3.5",
    "autoprefixer": "^10.3.0",
    "cssnano": "^5.0.6",
    "cssnano-preset-default": "^5.1.3",
    "eslint": "^7.30.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-import-resolver-node": "^0.3.4",
    "eslint-plugin-etc": "^1.5.4",
    "eslint-plugin-import": "^2.23.4",
    "eslint-plugin-only-warn": "^1.0.2",
    "eslint-plugin-prettier": "^3.4.0",
    "eslint-plugin-promise": "^5.1.0",
    "eslint-plugin-react": "^7.24.0",
    "eslint-plugin-regexp": "^0.13.1",
    "eslint-plugin-simple-import-sort": "^7.0.0",
    "eslint-plugin-sonarjs": "^0.9.1",
    "eslint-plugin-sort-class-members": "^1.11.0",
    "eslint-plugin-testing-library": "^4.9.0",
    "eslint-plugin-total-functions": "^4.9.0",
    "eslint-plugin-unicorn": "^34.0.1",
    "eslint-plugin-wix-editor": "^3.3.0",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^27.0.6",
    "jest-transform-stub": "^2.0.0",
    "postcss": "^8.3.5",
    "prettier": "^2.3.2",
    "tailwindcss": "^2.2.4",
    "ts-jest": "^27.0.3",
    "typescript": "^4.3.5",
    "vite": "^2.4.1",
    "vite-tsconfig-paths": "^3.3.13",
    "watchexec-bin": "^1.0.0"
  }
}
