{
  "name": "realworld-example-app",
  "private": true,
  "version": "0.0.0-dev",
  "scripts": {
    "dev": "vite",
    "dev:2": "run-p dev tscheck:w",
    "build": "tsc && vite build",
    "serve": "vite preview",
    "tscheck": "tsc --noEmit",
    "tscheck:w": "npm run tscheck -- --watch --preserveWatchOutput",
    "test": "npm run eslint && npm run tscheck && npm run test:cov",
    "test:r": "node node_modules/jest/bin/jest --runInBand --verbose",
    "test:w": "npm run test:r -- --watch",
    "test:cov": "npm run test:r -- --collectCoverage",
    "eslint": "node node_modules/eslint/bin/eslint \"src/**/*.{ts,tsx}\"",
    "eslint:w": "watchexec -w src \"npm run eslint\"",
    "eslint:fix": "npm run eslint -- --fix"
  },
  "dependencies": {
    "@hookform/resolvers": "^2.8.8",
    "ahooks": "^3.1.9",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.13.2",
    "clsx": "^1.1.1",
    "jwt-decode": "^3.1.2",
    "ky": "^0.28.7",
    "njct": "^8.1.0",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-hook-form": "^7.25.3",
    "react-router-dom": "^6.2.1",
    "recoil": "^0.6.1",
    "rsts": "^1.0.0"
  },
  "devDependencies": {
    "@kyleshevlin/eslint-plugin": "^1.3.0",
    "@testing-library/jest-dom": "^5.16.1",
    "@testing-library/react": "^12.1.2",
    "@types/jest": "^27.4.0",
    "@types/react": "^17.0.38",
    "@types/react-dom": "^17.0.11",
    "@types/react-router-dom": "^5.3.3",
    "@typescript-eslint/eslint-plugin": "^5.10.1",
    "@typescript-eslint/parser": "^5.10.1",
    "@vitejs/plugin-react-refresh": "^1.3.6",
    "autoprefixer": "^10.4.2",
    "cssnano": "^5.0.16",
    "cssnano-preset-default": "^5.1.11",
    "eslint": "^8.8.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-import-resolver-node": "^0.3.6",
    "eslint-plugin-etc": "^2.0.2",
    "eslint-plugin-import": "^2.25.4",
    "eslint-plugin-only-warn": "^1.0.3",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-promise": "^6.0.0",
    "eslint-plugin-react": "^7.28.0",
    "eslint-plugin-react-hooks": "^4.3.0",
    "eslint-plugin-regexp": "^1.5.1",
    "eslint-plugin-simple-import-sort": "^7.0.0",
    "eslint-plugin-sonarjs": "^0.11.0",
    "eslint-plugin-sort-class-members": "^1.14.1",
    "eslint-plugin-testing-library": "^5.0.4",
    "eslint-plugin-unicorn": "^40.1.0",
    "eslint-plugin-wix-editor": "^3.3.0",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^27.4.7",
    "jest-mock-extended": "^2.0.4",
    "jest-transform-stub": "^2.0.0",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.4.5",
    "prettier": "^2.5.1",
    "simplytyped": "^3.3.0",
    "tailwindcss": "^3.0.18",
    "ts-jest": "^27.1.3",
    "tsconfig-paths-jest-mapper": "^1.4.0",
    "typescript": "^4.5.5",
    "vite": "^2.7.13",
    "vite-tsconfig-paths": "^3.3.17",
    "watchexec-bin": "^1.0.0"
  }
}
